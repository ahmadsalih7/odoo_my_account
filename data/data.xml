<odoo>
    <data>
        <!-- Sequence for Journal Entries !-->
        <record id="sequence_journal_entries" model="ir.sequence">
            <field name="name">Journal Entries</field>
            <field name="code">myaccount.move</field>
            <field name="prefix">MISC/%(year)s/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>
        <!-- Sequence for Invoices !-->
        <record id="sequence_invoices" model="ir.sequence">
            <field name="name">Invoices</field>
            <field name="code">myaccount.move.invoices</field>
            <field name="prefix">INV/%(year)s/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>

        <!-- Sequence for Cash !-->
        <record id="sequence_cash" model="ir.sequence">
            <field name="name">Cash</field>
            <field name="code">myaccount.payment.cash</field>
            <field name="prefix">CSH1/%(year)s/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>
        <!-- Sequence for bank !-->
        <record id="sequence_bank" model="ir.sequence">
            <field name="name">Bank</field>
            <field name="code">myaccount.payment.bank</field>
            <field name="prefix">BNK1/%(year)s/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>

        <!-- Sequence for Customer invoices !-->
        <record id="sequence_customer_invoices" model="ir.sequence">
            <field name="name">Payments customer invoices sequence</field>
            <field name="code">account.payment.customer.invoice</field>
            <field name="prefix">CUST.IN/%(year)s/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>

        <!-- Sequence for Vendor Bills !-->
        <record id="sequence_vendor_bills" model="ir.sequence">
            <field name="name">Payments vendor bills sequence</field>
            <field name="code">account.payment.vendor.bills</field>
            <field name="prefix">BILL/%(year)s/</field>
            <field name="padding">4</field>
            <field eval="False" name="company_id"/>
        </record>
    </data>
</odoo>